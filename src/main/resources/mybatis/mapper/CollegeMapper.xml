<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chen.ems.core.mapper.CollegeMapper">
    <update id="putCollege" parameterType="com.chen.ems.core.model.CollegeVO">
        update ems_college set name = #{name},created_time = #{createdTime}
        where id=#{id}
    </update>
</mapper>